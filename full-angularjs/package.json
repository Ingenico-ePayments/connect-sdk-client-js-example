    "connect-sdk-client-js": ">=3.6.0 <4.0.0"
  },
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "concurrently": "^2.2.0",
    "lite-server": "^2.3.0",
    "node-sass": "^3.13.1"
  }
}
